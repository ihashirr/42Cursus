{
    "version": "2.0.0",
    "configurations": [
        {
            "name": "  Extension ",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/server 47257",
            "args": [47257],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "configurationProvider": "ms-vscode.cppdbg",
            "preLaunchTask": "build"
        }
    ],
    "tasks": [
        {
            "type": "cppbuild",
            "label": "build",
            "command": "make",
            "args": [],
            "options": {
                "cwd": "${fileDirname}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "Task generated by Debugger."
        }
    ]
}